<aura:component controller="SymptomSearchApexController">
    <aura:attribute name="Symptoms" type="Symptom__c[]" />
    <aura:handler event="aura:locationChange" action="{!c.locationChange}"/>
    <aura:registerEvent name="sendingEvent" type="c:sendingSymptomEvent" />
    
    <aura:if isTrue="{!not(empty(v.Symptoms))}">
        <aura:iteration items="{!v.Symptoms}" var="sym" indexVar="i">
            <div  data-index="{!i}" onclick="{!c.removeSym}" >
               <c:SymptomItem symptomId="{!sym.Id}"/>
            </div>
        </aura:iteration>
        <br />
        <lightning:button variant="destructive" label="Diagnose" title="Diagnose" onclick="{!c.sendSymptoms}"/>
    </aura:if>
</aura:component>